{
  "version": 2,
  "builds": [
    {
      "src": "app/api/slack-interactions.ts",
      "use": "@vercel/node"
    }
  ],
  "build": {
    "env": {
      "PRISMA_GENERATE_COMMAND": "npx prisma generate",
      "src": "package.json",
      "use": "@vercel/node",
      "config": {
        "maxLambdaSize": "50mb"
      }
    }
  },
  "routes": [
    {
      "src": "/api/slack-interactions",
      "dest": "/app/api/slack-interactions.ts"
    }
  ],
  "hooks": {
    "build": "prisma generate"
  }
}
